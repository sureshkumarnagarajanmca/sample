{
  // Customer module configs
  "app": {
    "hostname": "localhost",
    "port": 3004,
    "protocol": "http",
    "commit": "{COMMITSHA}",
    "processJobs": false //true: this server should also process jobs

    /*
      // WARNING!!!
      // FOLLOWING IS FOR DEVELOPMENT ENVIRONMENTS
      // OR FOR SITUATIONS NEEDING DEBUGGING SUPPORT
      "debug": "*,-APILoader,-bootstrap,-oauth2orize,-express:*,-send,-mquery,-gm,-thumbnail"
      "workerCount": 1, //number of workers to start
      "noCluster": false, //true: don't start cluster/workers, only one process will be running
      "ttlOnError": 5000, //number a seconds a server will run once it encounters an exception while serving requests
     */
  },

  "User": {
    "dbConfig": {
      "host": "localhost",
      "port": 27017,
      "dbName": "fadauth"
    },
    "credit": {
      "initialLimit": 100,
      // Set low for development
      "initialDays": 1
    },
    "logger": {
      "logstashhost": "localhost",
      "enablefilelogs": false,
      "applogs": {
        "level": "error"
      },
      "log_files_path":"/logs"
    }
  }
}